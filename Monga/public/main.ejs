<html>
	<head>
		<title>
			Monga
		</title>
		<!-- Latest compiled and minified CSS -->
		<link rel="stylesheet"
 			href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
			integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" 
			crossorigin="anonymous">
		<link rel="stylesheet" type="text/css" href="stylesheets/style.css">
	</head>
	<body>
		<% for(var i=0; i < pictures.length; i++) { %>
			<div>
				<a href="data:<%=pictures[i][1]%>;base64,<%=pictures[i][0]%>" download><img class="images" src="data:image/jpeg;base64,<%=pictures[i][0]%>"/></a>
			</div>
		<% } %>
		<% for(var i=0; i < files.length; i++) { %>
			<div>
				<a href="data:<%=files[i][1]%>;base64,<%=files[i][0]%>" download/><input type="button" style="width:30px"></input></a>
			</div>
		<% } %>


		<div>
			<a href="/upload">Upload a file</a>
		</div>
	</body>
</html>
